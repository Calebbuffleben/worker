{
  "name": "stream-worker",
  "version": "0.2.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:clean": "rm -rf dist && npm run build",
    "start": "npm cache clean --force && node dist/index.js",
    "postinstall": "npm run build",
    "dev": "ts-node --transpile-only src/index.ts",
    "smoke:r2": "ts-node --transpile-only src/smoke-r2.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.645.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^1.4.1",
    "@types/mime": "^2.0.3",
    "axios": "^1.7.9",
    "bullmq": "^5.8.1",
    "dotenv": "^16.4.5",
    "fluent-ffmpeg": "^2.1.3",
    "ioredis": "^5.4.1",
    "mime": "^3.0.0",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
